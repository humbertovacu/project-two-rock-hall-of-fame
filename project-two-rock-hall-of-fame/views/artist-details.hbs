
<h1>{{artist.name}}</h1>
  <img src="{{imageUrl}}" alt="{{name}}-picture" width="400" height="auto" />
  <h3>Origin:</h3>
  <p>{{artist.origin}}</p>
  <h3>Birthday:</h3>
  <p>{{birthday}}</p>
  {{#if artist.deathDate}}
    <h3>Death</h3>
    <p>{{artist.deathDate}}</p>
  {{/if}}
  {{#each artist.bands}}
    <h3>{{name}}</h3>
  {{/each}}
